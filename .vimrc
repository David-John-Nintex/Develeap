runtime! debian.vim
syntax on
source $VIMRUNTIME/colors/evening.vim

set nocompatible

set wildmenu
set showcmd

set ignorecase
set smartcase

set laststatus=2

set nu!
set ts=4
set expandtab

set list! listchars=space:.,tab:>_,precedes:<,trail:_,eol:$,extends:>
set sw=4

set fileencoding=utf8
set encoding=utf8


